      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "repository": "https://github.com/rucas/shh.git",
  "author": "rucas <lucas.rondenet@gmail.com>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "express": "^4.16.3",
    "husky": "^1.2.0",
    "jest": "^23.1.0",
    "pkg-ok": "^2.2.0",
    "standard": "^12.0.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.642.0",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "lru-cache": "^4.1.3",
    "ms": "^2.1.1"
  }
