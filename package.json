{
  "name": "@rucas/shh",
  "version": "1.0.1",
  "keywords": [
    "security",
    "aws",
    "express"
  ],
  "description": "AWS KMS Express Middleware",
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "yarn standard",
    "test": "yarn jest"
  },
  "standard": {
